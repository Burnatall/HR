<Window x:Class="SPLab.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SPLab.ViewModels" 
        Style="{StaticResource macWnd}"
        mc:Ignorable="d" WindowStartupLocation="CenterScreen"
        Title="Main Window - (1)" Height="350" Width="525">
    <Window.DataContext>
        <local:MainViewModel></local:MainViewModel>
    </Window.DataContext>
    <Grid>
        <Border BorderThickness="1" BorderBrush="Black" Margin="0,33,316.8,174.6"></Border>
        <Label Margin="114,43,326.8,252.6">Dividend</Label>
        <Label Margin="114,78,326.8,217.6">Denominator</Label>
        <Label Content="{Binding ResultOfDiv}" Margin="114,113,326.8,182.6"></Label>
        <TextBox Text="{Binding Dividend}" Margin="10,43,415.8,252.6"></TextBox>
        <TextBox Text="{Binding Denominator}" Margin="10,78,415.8,217.6"></TextBox>
        <Button Command="{Binding My}" Margin="10,113,415.8,182.6" Width="Auto"></Button>
    </Grid>
</Window>
